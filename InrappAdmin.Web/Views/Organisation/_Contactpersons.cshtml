@model InrappAdmin.Web.Models.ViewModels.OrganisationViewModels.OrganisationViewModel

<h4>Kontaktpersoner</h4>


<table class="table">
    <tr>
        <th>
            Namn
        </th>
        <th>
            E-postadress
        </th>
        <th>
            Mobilnummer
        </th>
        <th>
            Mobilnummer verifierat
        </th>
        <th>
            Aktiv från
        </th>
        <th>
            Aktiv tom
        </th>
        <th>
            Skapad 
        </th>
        <th>
            Skapad av
        </th>
        <th>
            Andrad
        </th>
        <th>
            Andrad Av
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.ContactPersons)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Namn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumberConfirmed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AktivFrom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AktivTom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkapadDatum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkapadAv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AndradDatum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AndradAv)
            </td>

            @*<td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ID })
            </td>*@
        </tr>
    }

</table>

