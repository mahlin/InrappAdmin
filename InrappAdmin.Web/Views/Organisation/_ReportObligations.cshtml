@model InrappAdmin.Web.Models.ViewModels.OrganisationViewModels.OrganisationViewModel

<h4>Uppgiftsskyldighet</h4>

<table class="table">
    <tr>
        <th>
            Delregister
        </th>
        <th>
            Skyldig från
        </th>
        <th>
            Skyldig tom
        </th>
        <th>
            Rapportera per enhet
        </th>
        <th>
            Skapad
        </th>
        <th>
            Skapad av
        </th>
        <th>
            Andrad
        </th>
        <th>
            Andrad Av
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model.ReportObligations)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AdmDelregister.Kortnamn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkyldigFrom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkyldigTom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RapporterarPerEnhet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkapadDatum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkapadAv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AndradDatum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AndradAv)
            </td>

            @*<td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ID })
            </td>*@
        </tr>
    }
    
    
</table>