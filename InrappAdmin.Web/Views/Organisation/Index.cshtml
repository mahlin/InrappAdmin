@using InrappAdmin.Web.Models.ViewModels
@model InrappAdmin.Web.Models.ViewModels.OrganisationViewModels.OrganisationViewModel
@{
    ViewBag.Title = "Organisation";
}


<script type="text/javascript">
    $(function () {
        $("#txtKommunkod").focus();
    });

    //$(document).ready(function () {
    //    alert("Kommunkod: " + $('#Kommunkod').val());
    //    //$("#txtKommunkod").focus();
    //});
</script>

<br /><br />
<div class="row">
    <div class="col-md-8 padding-top-25">
        <section id="orgForm">
            @using (Html.BeginForm("GetOrganisation", "Organisation", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
            {
                @Html.AntiForgeryToken()
                <h4>Administrera organisation</h4>
                <hr />
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="form-group">
                    @Html.LabelFor(m => m.Kommunkod, new { @class = "col-md-2 control-label" })
                    <div class="col-md-10">
                        @Html.TextBoxFor(m => m.Kommunkod, new { @class = "form-control", id = "txtKommunkod" })
                        @Html.ValidationMessageFor(m => m.Kommunkod, "", new { @class = "text-danger" })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Sök organisation" class="btn btn-default" />
                    </div>
                </div>
                <hr />
            }
        </section>
    </div>
</div>

@if (Model != null)
